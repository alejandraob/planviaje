{
  "name": "plan-viaje-backend",
  "version": "1.0.0",
  "description": "Backend API for Plan Viaje application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:api": "node tests/run-tests.js",
    "db:sync": "node src/scripts/syncDatabase.js",
    "db:reset": "node src/scripts/resetDatabase.js",
    "create-test-user": "node src/scripts/createTestUser.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all"
  },
  "keywords": ["travel", "expenses", "group-travel", "finance"],
  "author": "Plan Viaje Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "firebase-admin": "^12.0.0",
    "axios": "^1.6.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.0",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "pdfkit": "^0.13.0",
    "xlsx": "^0.18.5",
    "cloudinary": "^1.41.0",
    "mercadopago": "^2.0.0",
    "twilio": "^4.19.0",
    "@sendgrid/mail": "^7.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
